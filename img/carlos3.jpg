      for (var i = 1; i <= cookies.length; i++) {
        const index = cookies[i - 1].indexOf('=')
        if (index !== -1) {
          const cookie = cookies[i - 1].substring(0, index).trim();
          cookiesArray.push(cookie);
        }
      }
      return cookiesArray
    }
